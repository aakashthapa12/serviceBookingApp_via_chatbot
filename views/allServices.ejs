<!DOCTYPE html>
<html>
<head>
    <title>All Services</title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <div class="container">
        <h1>All Services</h1>
        <ul>
            <% for (const email in groupedServices) { %>
                <li>
                    <div class="service-item">
                        <strong>Provider Email:</strong> <%= email %> <br>
                        <a class="view-link" href="/service-provider/services?email=<%= email %>">View Provider Services</a>
                    </div>
                </li>
            <% } %>
        </ul>
    </div>
</body>
</html>


